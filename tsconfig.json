{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "jsx": "react-jsx",
        "jsxImportSource": "hono/jsx",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "moduleResolution": "node",
        "outDir": "./dist"
    }
}
